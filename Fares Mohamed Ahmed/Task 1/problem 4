#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num, i=0;
    scanf("%d", &num);
    int arr[num];
   
    while(i<=num) {
        scanf("%d",&arr[i]);
        i++;
    }
    i--;
    /* Write the logic to reverse the array. */
    while(i>=0)
    {
        printf("%d ",arr[i]);
        i--;
        }}
